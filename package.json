        "concurrently": "^7.0.0",
        "eslint": "8.6.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.28.0",
        "eslint-plugin-react-hooks": "^4.3.0",
        "prettier": "^2.5.1",
        "typescript": "^4.5.4"
    },
    "workspaces": [
        "main",
        "client"
